{
	"name": "webbookmarks",
	"displayName": "webBookmarks",
	"description": "Web Bookmarks for VSCODE.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openWebBookmarks"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openWebBookmarks",
				"title": "Open Web Bookmarks"
			}
		],
		"keybindings": [
			{
				"command": "extension.openWebBookmarks",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"chai": "^4.2.0",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"opn": "^5.4.0"
	}
}
