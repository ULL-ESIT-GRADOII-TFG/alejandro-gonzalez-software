{
	"name": "webbookmarks",
	"displayName": "Web Bookmarks",
	"description": "Web Bookmarks for VSCODE.",
	"author": {
		"name": "Alejandro Gonzalez Alonso"
	},
	"categories": [
		"Other"
	],
	"publisher": "alu0100997910",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ULL-ESIT-GRADOII-TFG/alejandro-gonzalez-software"
	},
	"activationEvents": [
		"onCommand:extension.openWebBookmarks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openWebBookmarks",
				"title": "Open Web Bookmarks"
			}
		],
		"keybindings": [
			{
				"command": "extension.openWebBookmarks",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"opn": "^5.4.0"
	}
}
